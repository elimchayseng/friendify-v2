{
  "rewrites": [
    {
      "source": "/callback",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [{
    "path": "/api/updateTrackOfDay",
    "schedule": "0 9 * * *"
  }],
  "env": {
    "CRON_SECRET": "@cron_secret"
  }
} 